{
  "manifest_version": 3,
  "name": "Cocomine VPN Extension",
  "description": "Cocomine VPN Extension use proxy",
  "version": "0.1.3",
  "icons": {
    "16": "16.png",
    "24": "24.png",
    "32": "32.png",
    "64": "64.png",
    "192": "192.png",
    "512": "512.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "192.png"
  },
  "permissions": [
    "proxy",
    "storage"
  ],
  "host_permissions": [
    "*://*.vpn.cocomine.cc/*",
    "http://localhost:3000/*"
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "*://*.vpn.cocomine.cc/*",
        "http://localhost:3000/*"
      ]
    }
  ]
}